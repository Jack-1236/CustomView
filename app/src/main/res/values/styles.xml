<?xml version="1.0" encoding="utf-8"?><resources>    <style name="AttributeStyle.AttributeName">        <item name="android:textSize">12sp</item>        <item name="text_color">@color/attribute_name</item>        <item name="android:gravity">right|center_vertical</item>        <item name="android:padding">1dp</item>    </style>    <style name="AttributeStyle.AttributeValue">        <item name="android:textSize">15sp</item>        <item name="text_color">@color/attribute_value</item>        <item name="android:gravity">left|center_vertical</item>        <item name="android:paddingStart">2dp</item>    </style>    <style name="AttributeStyle" parent="Widget.AppCompat.TextView">AttributeView</style></resources>